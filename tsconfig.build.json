{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"sourceMap": false,
		"inlineSources": false,
		"inlineSourceMap": false,
		"noEmit": false,
		"declaration": true,
		"stripInternal": true,
		"outDir": "lib",
		"rootDir": "src"
	},
	"include": ["src"],
	"exclude": ["test", "node_modules", "**/*.spec.ts", "**/*.test.ts"]
}
